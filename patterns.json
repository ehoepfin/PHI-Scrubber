{
    "person" : {
        "id" : "PERSON",
        "pattern": [
            [{"ENT_TYPE": "PERSON"}]
        ]
    },
    "email": {
        "id" : "EMAIL",
        "pattern" : [
            [{"LIKE_EMAIL": true, "OP" : "*"}]
        ]
    },
    "phone_number" : {
        "id" : "PHONE",
        "pattern": [
            [
                {"ORTH": "("}, {"SHAPE": "ddd"}, {"ORTH": ")"}, {"SHAPE": "ddd"},
                {"ORTH": "-", "OP": "?"}, {"SHAPE": "dddd"}
            ]
        ]
    },
    "ip" : {
        "id" : "IP",
        "pattern" : [
            [{"TEXT" : {"REGEX" : "\\d+(?:\\.\\d+){3}"}}]
        ]
    },
    "url" : {
        "id" : "URL",
        "pattern" : [
            [{"TEXT" : {"REGEX" : "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)?"}}]
        ]
    },
    "social_security_number" : {
        "id" : "SSN",
        "pattern": [
            [{"SHAPE": "ddd"},
            {"ORTH": "-", "OP": "?"}, {"SHAPE": "dd"},
            {"ORTH": "-", "OP": "?"}, {"SHAPE": "dddd"}]
        ]
    },
    "medical_record_number" : {
        "id" : "MRN",
        "pattern": [
            [{"SHAPE": "dd"},
            {"ORTH": "-", "OP": "?"},
            {"SHAPE": "dd"},
            {"ORTH": "-", "OP": "?"},
            {"SHAPE": "dd"}]
        ]
    }
}